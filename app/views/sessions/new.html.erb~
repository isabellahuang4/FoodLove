<h1>Log In</h1>
<p>Don't have an account? <%= link_to "Create one!", new_user_path %> </p>
<%= form_for(:session, url: login_path) do |f| %>

  <p>
    <%= f.label :email %> <br>
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.label :remember_me %>
    <%= f.check_box :remember_me %>
  </p>
  <p>
    <%= f.submit "Log In"%>
  </p>
<% end %>
